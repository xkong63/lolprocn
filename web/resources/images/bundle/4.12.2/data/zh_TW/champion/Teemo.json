ct":[" {{ e3 }} -> {{ e3NL }}"," {{ e1 }} -> {{ e1NL }}"]},"maxrank":5,"cooldown":[8,8,8,8,8],"cooldownBurn":"8","cost":[30,35,40,45,50],"costBurn":"30/35/40/45/50","effect":[null,[6,12,18,24,30],[65,90,115,140,165],[10,20,30,40,50]],"effectBurn":[null,"6/12/18/24/30","65/90/115/140/165","10/20/30/40/50"],"vars":[{"link":"spelldamage","coeff":0.3,"key":"a1"},{"link":"spelldamage","coeff":0.1,"key":"a2"}],"costType":"Passive","range":"self","rangeBurn":"self","image":{"full":"ToxicShot.png","sprite":"spell9.png","group":"spell","x":432,"y":48,"w":48,"h":48},"resource":"被動"},{"id":"BantamTrap","name":"毒性陷阱","description":"提摩使用放在他背包裡的蘑菇，設置一個會爆炸的劇毒陷阱。如果敵人踩到陷阱上，它會釋放出一團毒雲，對敵人減速並造成持續傷害。","tooltip":"使用一個儲存的蘑菇來設置陷阱，如果敵人踩到陷阱會引爆，將毒散發到附近的敵人，減緩{{ e2 }}% 的跑速，並造成 {{ e1 }} <span class=\"color99FF99\">（+{{ a1 }}）</span>魔法傷害，持續 4 秒。陷阱持續 {{ e5 }} 分鐘。<br><br>提摩每<span class=\"colorFFFFFF\">{{ f1 }}</span>秒找到一個蘑菇，但他一次只能攜帶 3 個。","leveltip":{"label":["傷害","減速百分比","魔力消耗","計時器"],"effect":[" {{ e1 }} -> {{ e1NL }}"," {{ e2 }} -> {{ e2NL }}%","{{ e3 }} -> {{ e3NL }} ","{{ f1 }} -> {{ f2 }} "]},"maxrank":3,"cooldown":[1,1,1],"cooldownBurn":"1","cost":[75,100,125],"costBurn":"75/100/125","effect":[null,[200,325,450],[30,40,50],[75,100,125],[30,26,22],[10,10,10]],"effectBurn":[null,"200/325/450","30/40/50","75/100/125","30/26/22","10"],"vars":[{"link":"spelldamage","coeff":0.5,"key":"a1"},{"link":"@cooldownchampion","coeff":[35.0,31.0,27.0],"key":"f1"},{"link":"@cooldownchampion","coeff":[31.0,27.0],"key":"f2"}],"costType":"Mana","range":[230,230,230],"rangeBurn":"230","image":{"full":"BantamTrap.png","sprite":"spell9.png","group":"spell","x":0,"y":96,"w":48,"h":48},"resource":"{{ cost }}魔力"}],"passive":{"name":"偽裝","description":"提摩短時間內靜止不動會進入不限時的隱形狀態，脫離隱形後會增加 40% 的攻速，並持續 3 秒。","image":{"full":"Teemo_Camouflage.png","sprite":"passive3.png","group":"passive","x":0,"y":0,"w":48,"h":48}},"recommended":[{"champion":"Teemo","title":"TeemoHA","type":"riot","map":"12","mode":"ARAM","priority":false,"bl